(defsrc
  r s o b n e g t pause
)

(deflayer main
  r s o b n e g t sldr
)


(defseq
    https (r s)
    brackets (s n)
    nq (n e)
    status (g s t)
)

(defvirtualkeys
    https (macro h t t p s S-; / /)
    brackets (macro S-[ S-] lft)
    nq (macro S-q)
    status (macro g i t spc s t a t u s ret)
)

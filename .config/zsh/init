colorscript random

autoload -Uz compinit
compinit

HISTFILE=~/.histfile
HISTSIZE=1000
SAVEHIST=1000

export KEYTIMEOUT=1 # Kill the lag between switching to VIM mode

bindkey -v

source "$HOME/.config/zsh/0zinit"
source "$HOME/.config/zsh/plugins"
source "$HOME/.config/zsh/alias"
source "$HOME/.config/zsh/9other"
